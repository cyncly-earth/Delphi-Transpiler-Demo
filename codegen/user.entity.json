{
  "name": "User",
  "route": "users",
  "fields": [
    { "name": "id", "type": "number", "required": true, "readonly": true },
    { "name": "firstName", "type": "string", "required": true },
    { "name": "email", "type": "string", "validators": ["email"] },
    { "name": "isActive", "type": "boolean", "default": true }
  ],
  "api": { "baseUrl": "/api/users" },
  "ui": { "style": "scss", "standalone": true },
  "outputDir": "src/app/generated/users"
}
